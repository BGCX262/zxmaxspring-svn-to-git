CREATE MEMORY TABLE ROLE(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,DESCRIPTION VARCHAR(64),NAME VARCHAR(20))
CREATE MEMORY TABLE USER_ROLE(USER_ID BIGINT NOT NULL,ROLE_ID BIGINT NOT NULL,PRIMARY KEY(USER_ID,ROLE_ID),CONSTRAINT FK143BF46A608D2196 FOREIGN KEY(USER_ID) REFERENCES APP_USER(ID),CONSTRAINT FK143BF46ABB625DB6 FOREIGN KEY(ROLE_ID) REFERENCES ROLE(ID))
